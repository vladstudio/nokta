{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "windows": ["*"],
  "remote": {
    "urls": ["http://localhost:*", "https://*"]
  },
  "permissions": [
    "core:default",
    "opener:default",
    "notification:default",
    "notification:allow-is-permission-granted",
    "notification:allow-request-permission",
    "notification:allow-notify",
    "store:default",
    "autostart:default"
  ]
}
